  "bugs": {
    "url": "https://github.com/Shealtiel-Li/node-mvc-template/issues"
  },
  "homepage": "https://github.com/Shealtiel-Li/node-mvc-template#readme",
  "dependencies": {
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "cls-hooked": "^4.2.2",
    "compression": "^1.7.4",
    "config": "^3.3.3",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "glob": "^7.1.6",
    "ioredis": "^4.19.2",
    "js-yaml": "^3.14.0",
    "lodash": "^4.17.20",
    "log4js": "^6.3.0",
    "pg": "^8.5.1",
    "pretty-error": "2.1.1",
    "redlock": "^4.2.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "sequelize-typescript": "^1.1.0",
    "ts-node": "^9.1.0",
    "typescript": "^4.1.2"
  },
  "devDependencies": {
    "@types/cls-hooked": "^4.3.1",
    "@types/compression": "^1.7.0",
    "@types/config": "^0.0.37",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.9",
    "@types/glob": "^7.1.3",
    "@types/ioredis": "^4.17.8",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.10",
